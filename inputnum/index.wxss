.inputNumView{width:200rpx;border:1px solid #ddd;line-height:1;position:relative;border-radius:2px;overflow:hidden;text-align:center;}
.inputNumView view{font-size:46rpx;display:block;color:#333;background:#f9f9f9;}
.inputNum-plus{width:58rpx;height:54rpx;line-height:48rpx;float:left;border-right:1px solid #ddd;position:relative;z-index:10;}
.inputNum-mins{width:58rpx;height:54rpx;line-height:48rpx;float:right;border-left:1px solid #ddd;position:relative;z-index:10;}
.inputNumView  input{height:100%;width:100%;line-height:1;min-height:inherit;position:absolute;z-index:8;left:0;top:0;font-size:30rpx;}
.inputNumView view.none{background:#f5f5f5;color:#ccc}
.inputNumView.disabled view{background:#f5f5f5;color:#ccc}
.inputNumView.disabled  input{color:#999}